<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.leadersante619457" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Leader Santé</name>
